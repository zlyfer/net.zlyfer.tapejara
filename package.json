{
  "name": "net.zlyfer.pteroapp",
  "version": "0.0.1",
  "author": "Frederik 'zlyfer' Shull",
  "homepage": "https://zlyfer.net",
  "description": "A Pterodactyl Panel App",
  "scripts": {
    "start": "npm run preBuild && ionic serve --no-open --configuration=development",
    "run-android": "ionic serve --no-open --configuration=app",
    "run-android_": "ionic cap run android -l --external --configuration=app",
    "run-ios": "ionic cap run ios -l --external --configuration=app",
    "run-freddy": "ionic cap run android -l --external --public-host=192.168.178.25 --target=2748e267 --configuration=app",
    "build-website": "npm run preBuild && npx ionic build --configuration=production && npx cap copy",
    "build-android": "npm run preBuild && npx ionic build --configuration=app.production && npx cap copy",
    "build-android_": "ionic build --configuration=app.production && ionic cap build android",
    "build-ios": "ionic build --configuration=app.production && ionic cap build ios",
    "generate-assets": "npx @capacitor/assets generate --android",
    "generate-assets_": "npx cordova-res --skip-config --copy",
    "buildWithTag": "node buildWithTag.js",
    "preBuild": "node preBuild.js",
    "eslint": "npx eslint src/app",
    "eslint-quiet": "npx eslint --quiet src/app",
    "eslint-fix": "npx eslint --fix src/app",
    "eslint-quiet-fix": "npx eslint --quiet --fix src/app"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.0",
    "@angular/common": "^18.0.0",
    "@angular/compiler": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/forms": "^18.0.0",
    "@angular/platform-browser": "^18.0.0",
    "@angular/platform-browser-dynamic": "^18.0.0",
    "@angular/router": "^18.0.0",
    "@capacitor/android": "6.1.0",
    "@capacitor/app": "6.0.0",
    "@capacitor/core": "6.1.0",
    "@capacitor/haptics": "6.0.0",
    "@capacitor/keyboard": "6.0.1",
    "@capacitor/status-bar": "6.0.0",
    "@fortawesome/angular-fontawesome": "^0.15.0",
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-regular-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@ionic/angular": "^8.0.0",
    "ionicons": "^7.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "websocket": "^1.0.35",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.0",
    "@angular-eslint/builder": "^18.0.0",
    "@angular-eslint/eslint-plugin": "^18.0.0",
    "@angular-eslint/eslint-plugin-template": "^18.0.0",
    "@angular-eslint/schematics": "^18.0.0",
    "@angular-eslint/template-parser": "^18.0.0",
    "@angular/cli": "^18.0.0",
    "@angular/compiler-cli": "^18.0.0",
    "@angular/language-service": "^18.0.0",
    "@capacitor/cli": "6.1.0",
    "@ionic/angular-toolkit": "^11.0.1",
    "@types/jasmine": "~5.1.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.1",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "jasmine-core": "~5.1.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.4.0"
  },
  "browserslist": [
    "Chrome >=79",
    "ChromeAndroid >=79",
    "Firefox >=70",
    "Edge >=79",
    "Safari >=14",
    "iOS >=14"
  ]
}
